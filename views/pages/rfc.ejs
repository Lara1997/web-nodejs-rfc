<%- contentFor('contact-form') %>
<div class="container contact-form">
    <div class="contact-image">
        <img src="https://image.ibb.co/kUagtU/rocket_contact.png" alt="rocket_contact" />
    </div>
    <form method="post">
        <h3>Ingresa Tus Datos</h3>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <input type="text" name="txtName" class="form-control" placeholder="Nombre"
                        style="text-transform:uppercase;" onkeyup="javascript:this.value=this.value.toUpperCase();" />
                </div>
                <div class="form-group">
                    <input type="text" name="txtPaterno" class="form-control" placeholder="Apellido Paterno"
                        style="text-transform:uppercase;" onkeyup="javascript:this.value=this.value.toUpperCase();"
                        value="" />
                </div>
                <div class="form-group">
                    <input type="text" name="txtMaterno" class="form-control" placeholder="Apellido Materno" value=""
                        style="text-transform:uppercase;" onkeyup="javascript:this.value=this.value.toUpperCase();" />
                </div>

                <div class="form-group">
                    <input type="date" name="fecha" class="form-control" placeholder="" value="" />
                </div>
                <!-- 
                <div class="form-group">
                    <input type="submit" name="btnSubmit" class="btnContact" value="Obtener RFC" />
                </div> -->
                <div class="input-group mb-3">
                    <div class="input-group-prepend">
                        <input class="btn btn-danger" type="button" value="Obtener RFC">
                    </div>
                    <input type="text" class="form-control" placeholder="" aria-label="Example text with button addon"
                        aria-describedby="button-addon1">
                </div>
            </div>
        </div>
</div>
</form>
</div>